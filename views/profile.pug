html
    head
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js")
        link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin="anonymous")
        link(rel="stylesheet" type="text/css" href="src/sconfiCustom.css")
        meta(name="viewport" content="width=device-width, initial-scale=1")
        meta(charset="utf-8")
            
    body
        include nav
        
        div(class="containerBackground container-fluid")
            div(class="normalBGMarginrow row")
                div(class="col-sm-12")
                    h1(id="titleProfilepage") Account #{name}
            br
            div(class="row")
                div(class="col-sm-3 col-sm-offset-2 btnDiv")
                    div(class="dropdown")
                        button(id="btnLeden" type="button" class="btn btn-default btn-lg dropdown-toggle" data-toggle="dropdown") UPDATE LEDEN
                        span(class="caret")
                        ul(class="dropdown-menu" aria-labelledby="btnLeden" role="menu")
                            li(role="presentation")
                                a(href="/addBatchPage" role="menuitem") Nieuwe lichting
                            li(role="presentation")
                                a(href="/updateLeden" role="menuitem") Update info leden
            br
            div(class="row")
                div(class="col-sm-3 col-sm-offset-2 btnDiv")
                    button(id="btnPreasidium" type="button" class="btn btn-default btn-lg") UPDATE PREASIDIUM
            br
            div(class="row")
                div(class="col-sm-3 col-sm-offset-2 btnDiv")
                    button(id="btnEvents" type="button" class="btn btn-default btn-lg") UPDATE LATEST EVENT
            br
            div(class="row")
                div(class="col-sm-3 col-sm-offset-2 btnDiv")
                    button(id="btnMaastricht" type="button" class="btn btn-default btn-lg") UPDATE MAASTRICHT




        include footer  
    script(src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa", crossorigin="anonymous")
